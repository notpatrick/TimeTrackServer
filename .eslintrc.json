{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "no-console": "off",
        "no-tabs": "warn",
        "no-param-reassign": "off",
        "one-var": "off",
        "eol-last": "off",
        "no-underscore-dangle": "off",
        "arrow-body-style": "warn",
        "arrow-parens": "warn"
    }
}